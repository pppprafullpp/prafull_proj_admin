<% obj = Doctor.find(@pending_action.key) %>
<% if obj.present? %>
   
    <div class="form-group"><label class="col-sm-2 control-label">Doctor Name:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.name %></span>
      </div>
    </div>
    

    <div class="form-group"><label class="col-sm-2 control-label">Email:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.email %></span>
      </div>
    </div>


    <div class="form-group"><label class="col-sm-2 control-label">Mobile:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.mobile %></span>
      </div>
    </div>

    <div class="form-group"><label class="col-sm-2 control-label">Landline:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.landline %></span>
      </div>
    </div>

    
<% end %>


