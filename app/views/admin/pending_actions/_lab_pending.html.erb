<% obj = Lab.find(@pending_action.key) %>
<% if obj.present? %>
    <% if obj.lab_id.present? %>
        <% parent_lab = Lab.find(obj.lab_id) %>
        <div class="form-group"><label class="col-sm-2 control-label">Parent Lab Name:</label>
          <div class="col-sm-10">
            <span class="form-control"><%= parent_lab.lab_name %></span>
          </div>
        </div>
    <% end %>

    <div class="form-group"><label class="col-sm-2 control-label">Lab Name:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.lab_name %></span>
      </div>
    </div>
    
    <div class="form-group"><label class="col-sm-2 control-label">Lab SEO Name:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.lab_seo_name %></span>
      </div>
    </div>

    <div class="form-group"><label class="col-sm-2 control-label">Email:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.email %></span>
      </div>
    </div>

    <div class="form-group"><label class="col-sm-2 control-label">Description:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.description %></span>
      </div>
    </div>

    <div class="form-group"><label class="col-sm-2 control-label">Contact Name:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.contact_name %></span>
      </div>
    </div>

    <div class="form-group"><label class="col-sm-2 control-label">City:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.city_id %></span>
      </div>
    </div>

    <div class="form-group"><label class="col-sm-2 control-label">Address:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.street_address %></span>
      </div>
    </div>

    <div class="form-group"><label class="col-sm-2 control-label">Mobile:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.mobile %></span>
      </div>
    </div>

    <div class="form-group"><label class="col-sm-2 control-label">Landline:</label>
      <div class="col-sm-10">
        <span class="form-control"><%= obj.landline %></span>
      </div>
    </div>

    
<% end %>


