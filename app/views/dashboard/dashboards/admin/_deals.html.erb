  <div class="animated fadeInUp">
    <div class="ibox ibox-title float-e-margins">
    <ul>
      <li class="active">
        <strong><a href="<%=create_deal_dashboard_dashboards_path%>" class="btn btn-w-m btn-primary">Create Deal</a></strong>
      </li>
     </ul>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <div class="ibox float-e-margins">
          <div class="ibox-title">
            <h5>Deal Details(<%=@deals.count%>)</h5>
            <div class="ibox-tools">
              <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
              </a>
              <a class="close-link">
                <i class="fa fa-times"></i>
              </a>
            </div>
          </div>
          <div class="ibox-content">
            <table class="table table-hover no-margins">
              <thead>
              <tr>
                <th>ID</th>
                <th>Category</th>
                <th>Provider</th>
                <th>Title</th>
                <th>Price</th>
                <th>Zipcodes</th>
                <th>Is Nationwide</th>
                <th>Type</th>
                <th>Start Date</th>
                <th>End Date</th>
                <th>Is Sponsered</th>
                <th>Action</th>
              </tr>
              <%@deals.each do |deal|%>
              <tr>
                <td><%=deal.id%></td>
                <td><%=deal.service_category.name%></td>
                <td><%=deal.service_provider.name%></td>
                <td><%=deal.title%></td>
                <td><%=deal.price%></td>
                <td>"NA"</td>
                <td><%=deal.is_nationwide%></td>
                <td><%=deal.deal_type%></td>
                <td><%=deal.start_date%></td>
                <td><%=deal.end_date%></td>
                <td><%=deal.is_sponsored%></td>
                <td>
                  <table>
                    <tr>
                      <td><a  data-toggle="tooltip" title="Edit"  href="<%=edit_deal_dashboard_dashboards_path(id:deal.id)%>"> <i  style="font-size:24px" class="fa fa-edit"></i></a></td>
                      <td><a  data-toggle="tooltip" title="Delete" href="<%= delete_deal_dashboard_dashboards_path(id:deal.id)%>"> <i  style="font-size:24px" class="fa fa-remove"></i></a></td>
                      <td><a  data-toggle="tooltip" title="Add Equipments" href="/add_equipments?id=<%=deal.id%>"> <i style="font-size:24px" class="fa fa-mobile"></i></a></td>
                      <td><a  data-toggle="tooltip" title="Edit Attributes" href="/edit_attributes?id=<%=deal.id%>"> <i style="font-size:24px" class="fa fa-tag"></i></a></td>
                    </tr>
                  </table>
                </td>
              </tr>
              <%end%>
              </thead>
            </div>
          </div>
        </div>
      </div>
    </div>
